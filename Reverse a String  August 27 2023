//C++

class Solution
{
    public:
    string reverseWord(string str)
    {
        // Your code goes here
        int j=str.size()-1;
        char temp;
        for(int i=0;i<j;i++,j--)
        {
            temp=str[i];
            str[i]=str[j];
            str[j]=temp;
        }
        return str;
    }
};
